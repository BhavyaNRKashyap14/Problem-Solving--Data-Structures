
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    
    while(t--)
    {
        int value;
        cin>>value;
        int curr_num =1;
        
        for(int i =1;i<=value;i++)
        {
            for(int j =1;j<=i;j++)
            {
                cout<<curr_num<<" ";
                curr_num++;
            }
            cout<<endl;
        }
        cout<<endl;
    }
   // write your code here
    return 0;
}

